 </div>\r\n            <div class=\"card-body\">\r\n                <table class=\"table\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>ID</th>\r\n                            <th>Nombre</th>\r\n                            <th>Marca</th>\r\n                            <th>Modelo</th>\r\n                            <th>Serial</th>\r\n                            <th>CC_Responsable</th>\r\n                            <th>Responsable</th>\r\n                            <th>Area</th>\r\n                            <th>Acciones</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"activo in activos\" v-bind:key=\"activo.id\">\r\n                            <td>{{activo.id}}</td>\r\n                            <td>{{activo.nombre}}</td>\r\n                            <td>{{activo.marca}}</td>\r\n                            <td>{{activo.modelo}}</td>\r\n                            <td>{{activo.serial}}</td>\r\n                            <td>{{activo.doc_resp}}</td>\r\n                            <td>{{activo.nom_resp}}</td>\r\n                            <td>{{activo.area}}</td>\r\n                            <td>\r\n                                <div class=\"btn-group\" role=\"group\" aria-label=\"\">\r\n                                    <router-link :to=\"{name:'Home' , params:{id: activo.id} }\" class=\"btn btn-success\" >Editar</router-link>\r\n                                    <button type=\"button\" v-on:click=\"borrarActivo(activo.id)\" class=\"btn btn-danger\">Borrar</button>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\r\n    data(){\r\n        return {\r\n            activos:[]